	fbloginEdit_4 : EditBox01;
	fbloginButton_5 : Button02;
	$waitRefresh : BOOL;
	END_VAR

	VAR
	res : USINT;
	pagID : UINT;
	n : USINT;
	iSP : INT;
	pDC : UDINT;
	rx1 : UINT;
	ry1 : UINT;
	rx2 : UINT;
	ry2 : UINT;
	END_VAR

	{CODE:ST}

	REPEAT

		(* Locking the video *)
		res := Video_Lock( res );

		(* Testing if page has changed *)
		pagID := guiCallStack[ giStackPtr ];

		IF pagID = 16#07 THEN

			(* the refresh task is arrived *)
			$waitRefresh := FALSE;

			(* *** REFRESH PAGE CONTROLS *** *)
			IF fbloginEdit_1.accMode <> kACS_MODIFY THEN
				fbloginEdit_1();
				(* Object: Edit_1 - Event: AfterUpdate - ProcName: check_password *)
				if login_password_1 = 1 and login_password_2 = 7 and login_password_3 = 0 and login_password_4 = 4 then login_visible_login := true ;
				end_if ;
				(* End of associated procedure *)
			END_IF;
			IF fbloginEdit_2.accMode <> kACS_MODIFY THEN
				fbloginEdit_2();
				(* Object: Edit_2 - Event: AfterUpdate - ProcName: check_password *)
				if login_password_1 = 1 and login_password_2 = 7 and login_password_3 = 0 and login_password_4 = 4 then login_visible_login := true ;
				end_if ;
				(* End of associated procedure *)
			END_IF;
			IF fbloginEdit_3.accMode <> kACS_MODIFY THEN
				fbloginEdit_3();
				(* Object: Edit_3 - Event: AfterUpdate - ProcName: check_password *)
				if login_password_1 = 1 and login_password_2 = 7 and login_password_3 = 0 and login_password_4 = 4 then login_visible_login := true ;
				end_if ;
				(* End of associated procedure *)
			END_IF;
			IF fbloginEdit_4.accMode <> kACS_MODIFY THEN
				fbloginEdit_4();
				(* Object: Edit_4 - Event: AfterUpdate - ProcName: check_password *)
				if login_password_1 = 1 and login_password_2 = 7 and login_password_3 = 0 and login_password_4 = 4 then login_visible_login := true ;
				end_if ;
				(* End of associated procedure *)
			END_IF;
			fbloginButton_5();

			(* *** REFRESHING BACKGROUND ITEMS *** *)
			(* Cursor across the call stack *)
			iSP := giStackPtr;

			(* *** REFRESHING PARENT BACKGROUND ITEMS *** *)

			
		END_IF;

		(* Unlocking the video *)
		res := Video_Unlock( res );

		(* Suspending the task *)
		res := Video_Sleep( 50 );

	UNTIL pagID <> 16#07
	END_REPEAT;

END_FUNCTION_BLOCK

